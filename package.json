{
  "name": "feedback-app-be",
  "author": "tibbs",
  "version": "1.0.0",
  "description": "Express app",
  "main": "./dist/listener.js",
  "repository": {
    "type": "git",
    "url": "git+test.git"
  },
  "bugs": {
    "url": "test/issues"
  },
  "homepage": "test#readme",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/listener.js",
    "setup-dbs": "psql -f ./db/setup.sql",
    "seed": "node ./dist/db/seeds/run-seed.js",
    "seed:test": "NODE_ENV=test npm run seed",
    "seed:dev": "NODE_ENV=development npm run seed",
    "seed:prod": "NODE_ENV=production npm run seed",
    "build": "tsc -p tsconfig.json && cp .env.* ./dist/",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:openHandles": "jest --detectOpenHandles"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.1",
    "nodemon": "^3.1.7",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.x",
    "@types/express": "^4.17.x",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.x.x",
    "@types/pg": "^8.11.10",
    "@types/pg-format": "^1.0.5",
    "@types/supertest": "^2.0.16",
    "supertest": "^7.0.0",
    "ts-jest": "^29.x.x",
    "ts-node": "^10.x.x",
    "typescript": "^5.x.x"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all",
      "jest-sorted"
    ]
